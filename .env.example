# Environment Configuration
ENV=development  # Options: development, production

# Prometheux API Configuration
PROMETHEUX_BASE_URL=https://api.prometheux.ai/jarvispy/solo/YOUR-USERNAME
PMTX_TOKEN=your_prometheux_token_here

# CORS Configuration
# Leave empty to use environment-based defaults
# Development: allows all origins (*)
# Production: uses CORS_ORIGINS_PROD if set, otherwise defaults below
# For custom origins, use comma-separated list: http://localhost:3000,https://your-frontend.vercel.app
CORS_ORIGINS=
CORS_ORIGINS_PROD=https://rocket-monitor.vercel.app,https://rocket-engine-monitor.vercel.app
CORS_ALLOW_CREDENTIALS=true
CORS_ALLOW_METHODS=*
CORS_ALLOW_HEADERS=*

# AWS S3 Credentials (for data sources)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
S3_BUCKET=your_s3_bucket

# PostgreSQL Database (for data sources)
POSTGRES_HOST=your_postgres_host
POSTGRES_PORT=5432
POSTGRES_DB=your_database
POSTGRES_USER=your_username
POSTGRES_PASSWORD=your_password
POSTGRES_TABLE=your_table

# MariaDB Database (for data sources)
MARIADB_HOST=your_mariadb_host
MARIADB_PORT=3306
MARIADB_DB=your_database
MARIADB_USER=your_username
MARIADB_PASSWORD=your_password
MARIADB_TABLE=your_table

# Neo4j Database (for data sources)
NEO4J_HOST=your_neo4j_host
NEO4J_PORT=7687
NEO4J_DB=neo4j
NEO4J_USER=neo4j
NEO4J_PASSWORD=your_password
NEO4J_NODE_TYPE=Employee
